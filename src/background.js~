var BackgroundLayer = cc.LayerColor.extend({
    ctor: function() {
        this._super();
    },

    init: function() {
//        this._super();
        this.initWithColor(new cc.Color4B(255,0,0,255));

        var logo = cc.Sprite.create(imgHeader);
        logo.setAnchorPoint(new cc.Point(0, 0));
        logo.setPosition(new cc.Point(0, 250));
        this.addChild(logo);
        debugger;
    }
});
var MenuScene = cc.Scene.extend({
    onEnter: function() {
        var layer = new MenuLayer();
        this.addChild(layer);
        layer.init();
    }
});
